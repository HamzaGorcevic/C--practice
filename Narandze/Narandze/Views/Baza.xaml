<UserControl x:Class="Narandze.Baza"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Narandze"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             
             >
    <Grid>
        <DataGrid x:Name="gridBaza">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Created at" Binding="{Binding CreatedAt}"/>
                <DataGridTextColumn Header="Columns" Binding="{Binding Columns}"/>
                <DataGridTextColumn Header="Broj VXC" Binding="{Binding Rows}"/>
                <DataGridTextColumn Header="Broj pokvarenih" Binding="{Binding Ruined}"/>
                <DataGridTextColumn Header="Broj zdravih" Binding="{Binding Healthy}"/>
                <DataGridTextColumn Header="Koleracija" Binding="{Binding Colleration}"/>

            </DataGrid.Columns>
        </DataGrid>
            
    </Grid>
</UserControl>
