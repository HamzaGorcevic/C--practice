<Window x:Class="Parking.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parking"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu>
            <MenuItem Header="Cenovcnik" Click="MenuItem_Click"/>
            <MenuItem Header="Vlasnici" Click="MenuItem_Click_1" />
            <MenuItem Header="Parking" Click="MenuItem_Click_2"/>
                
        </Menu>
        <StackPanel Grid.Row="1">
            <Frame x:Name="podloga"/>
        </StackPanel>
        
    </Grid>
</Window>
